model Section {
  id             Int          @id @unique @default(autoincrement())
  type           SECTION_TYPE @default(HERO)
  title          String?
  description    String?
  images         String[]     @default([])
  icons          String?
  link           String?
  ctaText        String?
  isVisible      Boolean?     @default(false)
  primaryColor   String?
  secondaryColor String?
  createdAt      DateTime     @default(now())
  updatedAt      DateTime     @updatedAt
}

